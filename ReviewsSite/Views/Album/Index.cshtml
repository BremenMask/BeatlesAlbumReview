@model IEnumerable<ReviewsSite.Models.Album> 
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Across the Universe</title>
</head>
<body>
    @foreach (var album in Model)
    {
        <p>Title: <span>@album.Name</span></p>
        <p>Description: <span>@album.Description</span></p>
        <p>Category: <span>@album.Category</span></p>
        <hr />
    }
</body>
</html>
